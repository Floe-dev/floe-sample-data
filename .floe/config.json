{
  "$schema": "https://unpkg.com/@floe/schema/1.0-alpha.1/schema.json",
  "tabs": [
    {
      "key": "changelog",
      "title": "Changelog"
    },
    {
      "key": "docs",
      "title": "Docs"
    }
  ],
  "structure": {
    "changelog": {
      "groupTitle": "Changelog",
      "type": "list",
      "pages": "./floe/changelog/*.md",
      "listBy": "date",
      "listDirection": "asc"
    },
    "docs": {
      "groupTitle": "Docs",
      "type": "stack",
      "pages": [
        "./floe/docs/welcome.md",
        {
          "groupTitle": "Getting Started",
          "type": "stack",
          "pages": [
            "./floe/docs/getting-started/usage.md",
            "./floe/docs/getting-started/installation.md"
          ],
          "listBy": "fileOrder",
          "listDirection": "asc"
        }
      ],
      "listBy": "fileOrder",
      "listDirection": "asc"
    }
  },
  "sidebar": {
    "./floe/docs/welcome.md": {
      "title:": "Welcome",
      "icon": "code",
      "iconStyle": "fill"
    }
  }
}
